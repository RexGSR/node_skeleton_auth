{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":"error",
        "eqeqeq":"error",
        "semi":["error", "always"],
        "quotes": ["error", "double"],
        "indent": ["error", 4]
    }
}
